{
    "version": 2,
    "builds": [
        {
            "src": "vercel.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "vercel.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "DJANGO_SETTINGS_MODULE": "select_class.settings"
    }
}
