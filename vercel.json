{
    "version": 2,
    "builds": [{
        "src": "vercel_app.py",
        "use": "@vercel/python"
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "vercel_app.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "DJANGO_SETTINGS_MODULE": "select_class.settings"
    }
}
