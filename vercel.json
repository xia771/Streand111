{
    "name": "select-class-app",
    "version": 2,
    "builds": [
        {
            "src": "vercel_app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9",
                "buildCommand": "pip install -r requirements.txt && python3 manage.py collectstatic --noinput"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "vercel_app.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "DJANGO_SETTINGS_MODULE": "select_class.settings",
        "DJANGO_SECRET_KEY": "django-insecure-=2%$&)4$%^&*+_)(*&^%$#@!~",
        "DEBUG": "False"
    }
}
