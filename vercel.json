{
    "version": 2,
    "builds": [
        {
            "src": "vercel.py",
            "use": "@vercel/python"
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "vercel.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "DJANGO_SETTINGS_MODULE": "select_class.settings"
    }
}
