{
    "name": "select-class-app",
    "version": 2,
    "builds": [
        {
            "src": "vercel.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "vercel.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "DJANGO_SETTINGS_MODULE": "select_class.settings",
        "DJANGO_SECRET_KEY": "django-insecure-=2%$&)4$%^&*+_)(*&^%$#@!~",
        "DEBUG": "False"
    }
}
